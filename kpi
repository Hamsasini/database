CREATE TABLE KPIs (
    KPIID INT PRIMARY KEY,
    CompanyID INT,
    Year INT,
    RevenueGrowthRate DECIMAL(10, 2),
    ProfitMargin DECIMAL(10, 2),
    ROI DECIMAL(10, 2),
    DebtToEquityRatio DECIMAL(10, 2),
    EPS DECIMAL(10, 2),
    MarketCapitalization DECIMAL(20, 2),
    CONSTRAINT FK_CompanyID_KPI FOREIGN KEY (CompanyID) REFERENCES Company(CompanyID)
);

-- Creating the FinancialData table
CREATE TABLE FinancialData (
    FinancialID INT PRIMARY KEY,
    CompanyID INT,
    Year INT,
    Revenue DECIMAL(20, 2),
    Expenses DECIMAL(20, 2),
    NetIncome DECIMAL(20, 2),
    Assets DECIMAL(20, 2),
    Liabilities DECIMAL(20, 2),
    Equity DECIMAL(20, 2),
    OperatingCashFlow DECIMAL(20, 2),
    InvestingCashFlow DECIMAL(20, 2),
    FinancingCashFlow DECIMAL(20, 2),
    CONSTRAINT FK_CompanyID FOREIGN KEY (CompanyID) REFERENCES Company(CompanyID)
);


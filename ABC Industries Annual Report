CREATE TABLE AnnualReport (
    ReportID INT PRIMARY KEY,
    CompanyID INT,
    Year INT,
    TotalRevenue DECIMAL(20, 2),
    NetIncome DECIMAL(20, 2),
    TotalAssets DECIMAL(20, 2),
    TotalLiabilities DECIMAL(20, 2),
    CONSTRAINT FK_CompanyID_AnnualReport FOREIGN KEY (CompanyID) REFERENCES Company(CompanyID)
);
